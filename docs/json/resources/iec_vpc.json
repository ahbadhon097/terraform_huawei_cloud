{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_iec_vpc": {
          "block": {
            "attributes": {
              "cidr": {
                "type": "string",
                "required": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "mode": {
                "type": "string",
                "optional": true,
                "forcenew": true,
                "default": "SYSTEM"
              },
              "name": {
                "type": "string",
                "required": true
              },
              "region": {
                "type": "string"
              },
              "subnet_num": {
                "type": "number",
                "computed": true
              }
            }
          },
          "product": {
            "short": "IEC",
            "name": "Intelligent EdgeCloud",
            "catalog": "Edge Cloud Services",
            "name_cn": "智能边缘云",
            "catalog_cn": "边缘云服务"
          }
        }
      }
    }
  }
}
